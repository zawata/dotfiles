{
    "clangd.detectExtensionConflicts": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.fontFamily": "FiraCode Nerd Font Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 13.5,
    "editor.guides.bracketPairs": true,
    "editor.inlayHints.enabled": "off",
    "editor.minimap.enabled": false,
    "editor.mouseWheelZoom": false,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [80,120],
    "editor.tabSize": 2,
    "editor.unicodeHighlight.allowedCharacters": {"Ô∏è": true},
    "emmet.showAbbreviationSuggestions": false,
    "eslint.alwaysShowStatus": true,
    "eslint.format.enable": true,
    "eslint.onIgnoredFiles": "warn",
    "eslint.packageManager": "yarn",
    "eslint.probe": ["javascript","javascriptreact","html","vue","markdown"],
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "onFocusChange",
    "git.ignoreLegacyWarning": true,
    "git.mergeEditor": false,
    "gitkraken.env": "staging",
    "gitlens.codeLens.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.validate.enable": false,
    "oneDarkPro.vivid": true,
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "crash",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.autoDetectColorScheme": true,
    "window.dialogStyle": "custom",
    "window.menuBarVisibility": "visible",
    "workbench.enableExperiments": true,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.layoutControl.enabled": false,
    "workbench.preferredDarkColorTheme": "Electron vue darker",
    "workbench.preferredLightColorTheme": "Quiet Light",
    "workbench.startupEditor": "none",
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "workbench.colorTheme": "Electron vue darker",
}